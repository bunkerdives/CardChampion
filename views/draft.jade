    .row-fluid
      #control-bar.span12
        #info.span3
          span Pack
          span.digital-clock  3
          span Pick
          span.digital-clock 14
        #timer.span3
          span.digital-clock 1:30
        #sort.span3
          .input-prepend
            #sort-wrapper.btn-success
              .btn-group
                button#sort-button.btn.btn-success.dropdown-toggle(data-toggle="dropdown")
                  i.icon-refresh.icon-white
                  span Sort
                ul.dropdown-menu
                  li
                    a(tabindex="-1", href="#") Pack
                  li
                    a(tabindex="-1", href="#") Picks
              span#prependedInput
                select
                  option by color
                  option by cost
                  option by type
                  option by rarity
        #shuffle.span3
          button#shuffle-button.btn.btn-danger(onclick="Draft.shufflePack();")
            i.icon-random.icon-white
            span Shuffle pack
